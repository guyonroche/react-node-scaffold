    "commander": "^2.9.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "url": "^0.11.0"
  },
